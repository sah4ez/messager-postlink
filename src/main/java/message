package postlink;

option java_package = "su.postlink.protoc";
option java_outer_classname = "Message";

message Body{
        required int64 date = 1;
        required string body = 2;

        required int32 idFrom = 3;
        required string nickNameFrom = 4;

        required int32 idTo = 6;
        required string nickNameTo = 7;
}



